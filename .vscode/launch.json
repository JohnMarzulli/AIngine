    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch DT Tool",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Tools/DecisionTreeTool/bin/Debug/netcoreapp3.0/DecisionTreeTool",
            "args": ["${workspaceFolder}/Data/weather.examples"],
            "cwd": "${workspaceFolder}/Tools/DecisionTreeTool/bin/Debug/netcoreapp3.0/",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Tools/DecisionTreeTool/bin/Debug/netcoreapp3.0/DecisionTreeTool.dll",
            "args": [
                "/home/jmarzulli/git/AIngine/Data/weather.examples"
            ],
            "cwd": "${workspaceFolder}/Tools/DecisionTreeTool",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}